public class CD implements Packable{
    private String artist;
    private String cd;
    private int year;
    
    public CD(String artist, String cd, int year){
        this.artist = artist;
        this.cd = cd;
        this.year = year;
    }

    @Override
    public double weight() {
        return 0.1;
    }
    @Override
    public String toString(){
        return artist + ": " + cd + " (" + year + ")";
    }
    
}
